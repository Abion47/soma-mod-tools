{
  "name": "soma-mod-tools",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Scripting & Modding support for SOMA",
  "keywords": [],
  "repository": "https://github.com/apjjm/soma-mod-tools",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "symbols-view:toggle-project-symbols"
    ],
    "atom-text-editor": [
