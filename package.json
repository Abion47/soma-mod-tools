      "symbols-view:return-from-declaration",
      "symbols-view:toggle-file-symbols"
    ]
  },
  "dependencies": {
    "async": "^0.2.6",
    "atom-space-pen-views": "^2.0.0",
    "ctags": "^3.0.0",
    "fs-plus": "^2.0.0",
    "fuzzaldrin": "^2.1.0",
    "humanize-plus": "1.4.2",
    "temp": "~0.8.1"
  }
}
